{
  "rewrites": [
    { "source": "/login", "destination": "/client/auth/login.html" },
    { "source": "/signup", "destination": "/client/auth/login.html" },
    { "source": "/register", "destination": "/client/auth/login.html" },
    { "source": "/dashboard", "destination": "/client/dashboard.html" },
    { "source": "/client/dashboard", "destination": "/client/dashboard.html" },
    { "source": "/admin/login", "destination": "/admin/auth/login.html" },
    { "source": "/admin/dashboard", "destination": "/admin/dashboard.html" },
    { "source": "/wip", "destination": "/pages/wip.html" },
    { "source": "/about", "destination": "/pages/aboutus.html" },
    { "source": "/contact", "destination": "/pages/contactus.html" },
    { "source": "/privacy", "destination": "/pages/privacy-policy.html" },
    { "source": "/terms", "destination": "/pages/terms.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/shared/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
